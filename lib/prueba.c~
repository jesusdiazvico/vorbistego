#include <stdio.h>
#include <stdlib.h>
#include <string.h>
#include <time.h>

#define MAXITERS 100
#define CATCH 50

int main() {

  int32_t i32=0;
  u_int32_t ui32=0;

  i32 |= (1 << 31) + 1;
  ui32 |= (1 << 31) + 1;
  fprintf(stderr, "i32: %d, ui32: %u\n", i32, ui32);
  return 0;

}

